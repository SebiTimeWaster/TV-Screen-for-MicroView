// screen buffer that can hold a whole screen
uint8_t screenBuffer[384];
  
// holds last random numbers to avoid showing same stripe twice
byte randomStripes[6];

// temp hold stripe number
byte tempStripe;

void setup() {
  // init microview
  uView.begin();
  uView.clear(PAGE);
  // init random generator
  randomSeed(analogRead(0));
}

void loop() {
  // fill screen buffer from random progmem segments
  for(byte x = 0; x < 6; x++) {
    // get random number for stripe, avoid showing the same stripe twice
    do {
      tempStripe = random(30);
    } while(randomStripes[x] == tempStripe);
    randomStripes[x] = tempStripe;
    // copy stripe data from progmem
    for(byte y = 0; y < 64; y++) {
      screenBuffer[x * 64 + y] = pgm_read_byte(&(screenStripes[randomStripes[x]][y]));
    }
  }
  // send data to display
  uView.displayScreen(screenBuffer);
  delay(fpsDelay);
}

